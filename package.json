{
  "name": "semantic-web-components",
  "version": "1.0.0",
  "description": "Semantic Web Components",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "webpack",
    "start": "webpack-dev-server --colors --progress --content-base src/ --hot",
    "dev-build": "webpack -d --display-modules --display-error-details --progress --colors",
    "dist-build": "webpack -p --display-modules --progress --colors",
    "deploy": "npm run dist-build && gh-pages -d dist -m \"Updates --skip-ci\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/perjo927/SemanticWebComponents.git"
  },
  "keywords": [
    "angular",
    "css modules",
    "typescript",
    "webpack",
    "semantic",
    "web",
    "components"
  ],
  "author": "Per Jonsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perjo927/SemanticWebComponents/issues"
  },
  "homepage": "https://github.com/perjo927/SemanticWebComponents#readme",
  "dependencies": {
    "angular": "^1.4.7",
    "angular-aria": "^1.4.7",
    "angular-ui-router": "^0.2.15"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.29",
    "babel-core": "^5.8.29",
    "babel-loader": "^5.3.2",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.21.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.5.0",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.2",
    "jshint": "^2.8.0",
    "jshint-loader": "^0.8.3",
    "ng-annotate-loader": "0.0.10",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "node-sass": "^3.4.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.5.6",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
